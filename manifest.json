{
  "name": "Find Me Time",
  "version": "0.4",
  "manifest_version": 2,
  "description": "Schedule your booksmarks on to your calendar",
  "homepage_url": "http://findmetime.io",
  "icons": {
    "16": "icons/fmt4.png",
    "48": "icons/fmt4.png",
    "128": "icons/fmt4.png"
  },
  "externally_connectable": {
      "matches": ["*://localhost/*"]
  },
  "background": {
    
    "scripts": ["./env.js","scripts/auth0chrome.min.js", "./main.js","scripts/jwt-decode.js"],
    "persistent": true
  },
  "permissions": [
    "notifications",
    "cookies",
    "tts",
    "tabs",
    "http://*/*",
    "storage",
    "identity",
    "identity.email",
    "https://apis.google.com/*",
    "https://*.auth0.com/*"
  ],
  "content_security_policy": "script-src 'self'  https://apis.google.com; object-src 'self'",
  
  
  "browser_action" : {
    "default_icon" : "icons/fmt4.png",
    "default_title" : "You have scheduled!",
    "default_popup" : "popup/popup.html"
  },
  "web_accessible_resources": ["icons/background.jpg", "icons/meteorite.png", "icons/unknown.png", "icons/claim.png"]
}
